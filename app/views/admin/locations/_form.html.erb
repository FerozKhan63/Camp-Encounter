<%= form_with(model: [:admin, @location] ) do |f| %>
  <%= render 'shared/errors', object: @location %>

  <div class="row">
    <div class="col-md-6 mb-4">
      <%= f.label :name %><br />
      <%= f.text_field :name, autofocus: true, placeholder: "Kenya", class: "form-control" %>
    </div>
  </div>
 
  <div class="actions">
    <%= f.submit class: "btn btn-success" %>
  </div>
<% end %>
